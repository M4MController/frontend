<div class="cardStyle">
  <table class="table text-default">
    <thead>
    <tr>
      <th>{{t 'components.sensor-card.obd.column_1'}}</th>
      <th>{{t 'components.sensor-card.obd.column_2'}}</th>
    </tr>
    </thead>
    <tbody>
    {{#each-in sensor.value as |key value|}}
      {{#if (not-eq value null)}}
        <tr>
          <td>{{t (concat 'components.sensor-card.obd.' key)}}</td>
          <td>{{value}}</td>
        </tr>
      {{/if}}
    {{/each-in}}
    </tbody>
  </table>
</div>