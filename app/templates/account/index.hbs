{{#cards/cards-container}}
  {{#each model as |object|}}
    {{cards/object-card object=object}}
  {{/each}}
  {{cards/plus-card
    text="Add new object"
    width=4
    click=(action "set" this "modalAddVisible" true)
  }}
{{/cards/cards-container}}

{{#if modalAddVisible}}
  {{modals/add-object
    onClose=(action "set" "modalAddVisible" false)
    onAdd=(action "onAddObjectAction")
    loading=modalAddLoading
  }}
{{/if}}
